package com.web.epictrip.controller;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.http.ResponseEntity;
import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RequestParam;
import org.springframework.web.bind.annotation.ResponseBody;
import org.springframework.web.bind.annotation.RestController;

import com.web.epictrip.service.Schedule_service;
import com.web.epictrip.vo.Schedule_Place_dt;

@Controller
public class Schedule_controller {

	@Autowired
	private Schedule_service service; 
	
	@RequestMapping("schedule")
	public String schedule() {
		return "CommunityDetail";
	}
	
	
	
	@GetMapping("schedule_Dt")
	@ResponseBody
	public ResponseEntity<List<Schedule_Place_dt>> schedule_Dt(
			@RequestParam("schedule_key") String schedule_key){
		
		return ResponseEntity.ok(service.schedule_Dt(schedule_key));
	}
	
	
